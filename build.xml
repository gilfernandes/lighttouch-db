<project basedir="." name="buildHelp" default="buildHelp">
    <target name="buildHelp">
        <zip destfile="help.zip">
            <fileset dir="web-app/help" />
        </zip>
    </target>
	
	<target name="extractSvn">
		<mkdir dir="../nosvnFormCreator" />
